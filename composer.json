{
  "name": "brightnucleus/injector",
  "description": "Config-driven extension of Auryn Dependency Injector.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "~4",
    "php-composter/php-composter-phpcs-psr2": ">=0.2.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alain Schlesser",
      "email": "alain.schlesser@gmail.com"
    }
  ],
  "require": {
    "brightnucleus/exceptions": ">=0.2",
    "brightnucleus/config": ">=0.3.1",
    "rdlowrey/auryn": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "BrightNucleus\\Injector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrightNucleus\\Injector\\Test\\": "tests/"
    },
    "files": ["tests/fixtures/TestClasses.php"]
  }
}
